a = int(input('Введите первое число '))
b = str(input('Тип операции '))
c = int(input('Введите второе число '))
x = 0
if c == 0:
    print ('На 0 не делится')
else:
    if b == '+':
        x = a + c
    elif b == '-':
        x = a - c
    elif b == '*':
        x = a * c
    elif b == '/':
        x = a / c
    else:
        print('error')
print(x)
